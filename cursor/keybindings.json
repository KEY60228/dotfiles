// 既定値を上書きするには、このファイル内にキー バインドを挿入しますauto[]
[
    {
        "key": "alt+tab",
        "command": "workbench.action.quickSwitchWindow"
    },
    {
        "key": "cmd+n",
        "command": "explorer.newFile"
    },
    {
        "key": "shift+cmd+n",
        "command": "explorer.newFolder"
    },
    {
        "key": "shift+ctrl+g",
        "command": "git-graph.view"
    },
    {
        "key": "shift+cmd+g",
        "command": "workbench.view.scm"
    },
    {
        "key": "cmd+f12",
        "command": "editor.action.goToImplementation",
        "when": "editorHasImplementationProvider && editorTextFocus"
    },
    {
        "key": "cmd+f12",
        "command": "-editor.action.goToImplementation",
        "when": "editorHasImplementationProvider && editorTextFocus"
    },
    {
        "key": "shift+cmd+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    },
    {
        "key": "cmd+j",
        "command": "terminal.focus"
    },
    {
        "key": "cmd+j",
        "command": "workbench.action.closePanel",
        "when": "panelFocus"
    },
    {
        "key": "cmd+down",
        "command": "-search.action.openResult",
        "when": "fileMatchOrMatchFocus && searchViewletVisible"
    },
    {
        "key": "alt+cmd+g c",
        "command": "gitlens.views.searchAndCompare.focus"
    },
    {
        "key": "delete",
        "command": "gitlens.views.searchAndCompare.clear",
        "when": "focusedView == 'gitlens.views.searchAndCompare'"
    },
    {
        "key": "cmd+enter",
        "command": "gitlens.views.searchAndCompare.selectForCompare",
        "when": "focusedView == 'gitlens.views.searchAndCompare'"
    },
    {
        "key": "alt+cmd+g n",
        "command": "diffEditor.switchSide"
    },
    {
        "key": "cmd+w",
        "command": "-workbench.action.closeWindow",
        "when": "!editorIsOpen && !multipleEditorGroups"
    }
]